{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}
